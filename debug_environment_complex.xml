  </link>
  
  <!-- Hanging obstacle -->
  <link name="hanging_obstacle">
    <visual>
      <geometry>
        <box size="0.3 0.3 0.1"/>
      </geometry>
      <origin xyz="0.7 0.2 1.3" rpy="0 0 0"/>
      <material name="plastic">
        <color rgba="1.0 0.5 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.3 0.3 0.1"/>
      </geometry>
      <origin xyz="0.7 0.2 1.3" rpy="0 0 0"/>
    </collision>
  </link>
  
  <!-- Ground plane -->
  <link name="ground">
    <visual>
      <geometry>
        <box size="4.0 4.0 0.02"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <material name="floor">
        <color rgba="0.3 0.3 0.3 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="4.0 4.0 0.02"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    </collision>
  </link>
  
</robot>
